{
  "name": "react-aptor",
  "version": "1.2.1",
  "private": false,
  "description": "React API connector",
  "keywords": [
    "api",
    "hook",
    "react",
    "aptor",
    "apiHandler",
    "reactApiHandler",
    "thirdParty"
  ],
  "homepage": "https://github.com/realamirhe/react-aptor#readme",
  "bugs": {
    "url": "https://github.com/realamirhe/react-aptor/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/realamirhe/react-aptor"
  },
  "license": "MIT",
  "author": {
    "name": "Amir.H Ebrahimi",
    "email": "amirhosseinebrahimi77@gmail.com"
  },
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "lib/",
    "esm/",
    "src/"
  ],
  "scripts": {
    "prebuild": "yarn clean",
    "build": "yarn build:cjs && yarn build:es",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "clean": "rimraf lib esm coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "yarn lint --fix",
    "lint:types": "tsc --noEmit",
    "prepare": "husky install",
    "test": "jest --coverage=false",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --coverage=false --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/react": "^13.1.1",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "fork-ts-checker-webpack-plugin": "^7.2.9",
    "husky": "^7.0.4",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.0.2",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-test-renderer": "^18.1.0",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.55.0",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
