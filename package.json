{
  "name": "react-aptor",
  "version": "1.3.1",
  "private": false,
  "description": "React API connector",
  "keywords": [
    "api",
    "hook",
    "react",
    "aptor",
    "apiHandler",
    "reactApiHandler",
    "thirdParty"
  ],
  "homepage": "https://github.com/realamirhe/react-aptor#readme",
  "bugs": {
    "url": "https://github.com/realamirhe/react-aptor/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/realamirhe/react-aptor"
  },
  "license": "MIT",
  "author": {
    "name": "Amir.H Ebrahimi",
    "email": "amirhosseinebrahimi77@gmail.com"
  },
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "files": [
    "lib/",
    "esm/",
    "src/"
  ],
  "scripts": {
    "prebuild": "yarn clean",
    "build": "yarn build:cjs && yarn build:es",
    "build:cjs": "tsc",
    "build:es": "tsc -m esNext --outDir esm",
    "clean": "rimraf lib esm",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "yarn lint --fix",
    "lint:types": "tsc --noEmit",
    "prepare": "husky install",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "7.13.14",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.13.12",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.13.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.1",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "eslint": "7.23.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "fork-ts-checker-webpack-plugin": "6.2.0",
    "husky": "^7.0.4",
    "jest": "^27.2.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "3.0.2",
    "ts-jest": "^27.0.5",
    "ts-loader": "8.1.0",
    "ts-node": "9.1.1",
    "typescript": "4.1.5"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
